{
  "name": "@sylphx/cat-tracing",
  "version": "0.0.0",
  "description": "W3C Trace Context support for @sylphx/cat logger",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm --sourcemap=external && tsc --declaration --emitDeclarationOnly --outDir dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "cat",
    "logger",
    "tracing",
    "w3c",
    "trace-context",
    "distributed-tracing",
    "opentelemetry"
  ],
  "author": "Kyle Zhu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SylphxAI/cat",
    "directory": "packages/cat-tracing"
  },
  "peerDependencies": {
    "@sylphx/cat": "^0.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
